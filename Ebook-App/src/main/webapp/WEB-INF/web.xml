<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.user.servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddUsercontroller</servlet-name>
  	<display-name>AddUsercontroller</display-name>
  	<description></description>
  	<servlet-class>eba.controller.AddUsercontroller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginUsercontroller</servlet-name>
  	<display-name>LoginUsercontroller</display-name>
  	<description></description>
  	<servlet-class>eba.controller.LoginUsercontroller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BooksAdd</servlet-name>
  	<display-name>BooksAdd</display-name>
  	<description></description>
  	<servlet-class>eba.controller.BooksAdd</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddUsercontroller</servlet-name>
  	<url-pattern>/AddUsercontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginUsercontroller</servlet-name>
  	<url-pattern>/LoginUsercontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BooksAdd</servlet-name>
  	<url-pattern>/BooksAdd</url-pattern>
  </servlet-mapping>
</web-app>
